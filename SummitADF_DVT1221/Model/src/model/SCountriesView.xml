<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="SCountriesView"
  Version="12.1.3.7.3"
  InheritPersonalization="true"
  SelectList="SCountries.ID, 
       SCountries.COUNTRY, 
       SCountries.REGION_ID, 
       SCountries.COUNTRY_CODE"
  FromList="S_COUNTRIES SCountries"
  BindingStyle="OracleName"
  CustomQuery="false"
  PageIterMode="Full">
  <EntityUsage
    Name="SCountries"
    Entity="model.SCountries"/>
  <ViewAttribute
    Name="Id"
    IsNotNull="true"
    PrecisionRule="true"
    EntityAttrName="Id"
    EntityUsage="SCountries"
    AliasName="ID"/>
  <ViewAttribute
    Name="Country"
    IsUnique="true"
    PrecisionRule="true"
    EntityAttrName="Country"
    EntityUsage="SCountries"
    AliasName="COUNTRY"/>
  <ViewAttribute
    Name="RegionId"
    PrecisionRule="true"
    EntityAttrName="RegionId"
    EntityUsage="SCountries"
    AliasName="REGION_ID"/>
  <ViewAttribute
    Name="WarehouseCount"
    IsUpdateable="false"
    IsSelected="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName="VIEW_ATTR"
    SQLType="INTEGER">
    <TransientExpression><![CDATA[SWarehouseView.count('Id')]]></TransientExpression>
    <RecalcCondition><![CDATA[true]]></RecalcCondition>
  </ViewAttribute>
  <ViewAttribute
    Name="CountryCode"
    PrecisionRule="true"
    EntityAttrName="CountryCode"
    EntityUsage="SCountries"
    AliasName="COUNTRY_CODE"/>
  <ViewAttribute
    Name="TotalSales"
    IsUpdateable="false"
    IsSelected="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Double"
    ColumnType="DOUBLE PRECISION"
    AliasName="VIEW_ATTR"
    SQLType="DOUBLE">
    <TransientExpression
      trustMode="untrusted"><![CDATA[SCustomerView.sum ('TotalSales')]]></TransientExpression>
    <RecalcCondition
      trustMode="untrusted"><![CDATA[true]]></RecalcCondition>
  </ViewAttribute>
  <ViewAttribute
    Name="TotalOrders"
    IsUpdateable="false"
    IsSelected="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName="VIEW_ATTR"
    SQLType="INTEGER">
    <TransientExpression
      trustMode="untrusted"><![CDATA[SCustomerView.count('Id')]]></TransientExpression>
    <RecalcCondition
      trustMode="untrusted"><![CDATA[true]]></RecalcCondition>
  </ViewAttribute>
  <ViewLinkAccessor
    Name="SWarehouseView"
    ViewLink="model.CountryWarehouseViewLink"
    Type="oracle.jbo.RowIterator"
    IsUpdateable="false"/>
  <ViewLinkAccessor
    Name="SCustomerView"
    ViewLink="model.CountryCustomerViewLink"
    Type="oracle.jbo.RowIterator"
    IsUpdateable="false"/>
</ViewObject>
